extends layout

block content
  table.powerSet
    tbody
      tr
        td Super Hero Power
        td #{powerSet.name}
      tr
        td(colspan=2) #{powerSet.openText}
      tr
        td(colspan=2) #{powerSet.abilityText}
      tr
        td(colspan=2) #{abMods}
      tr
        td(colspan=2) #{powerSet.powersText}
  each list, index in powers
    div(id = 'powersTable'+index)
  script.
    let powers = !{JSON.stringify(powers)};



